{
    "manifest_version": 2,
    "name": "Bouncing Image",
    "description": "This extension bounces an image around your screen.",
    "version": "1.0",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["bounce.js"]
      }
    ]
  }



